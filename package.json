  "bugs": {
    "url": "https://github.com/PhuSon2809/trello_clone_api/issues"
  },
  "homepage": "https://github.com/PhuSon2809/trello_clone_api#readme",
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "nodemon": "^2.0.20"
  }
}
